# CCFD build configuration file

# select compiler [gnu, intel]
COMPILER = gnu

# select equation system [euler, navierstokes]
EQNSYS = euler

# multithreading flag [on, off]
PARALLEL = on

# debugging flag [on, off]
DEBUG = off

# profiling flag [on, off]
PROF = off

# CGNS library setup [3.4.1, 4.1.1]
CGNS_VERSION = 4.1.1
